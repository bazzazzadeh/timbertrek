{
  "name": "timbertrek",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:r": "rollup -c",
    "dev:r": "rollup -c -w",
    "start:r": "sirv public --no-clear --port 5005",
    "dev": "vite",
    "build": "vite build",
    "build:lite": "source publish-lite.sh",
    "build:notebook": "vite build --mode notebook",
    "build:github": "vite build --mode github",
    "publish:gh": "pnpm run build:github && pnpm run build:lite && source publish-gh.sh",
    "publish:vercel": "source publish-vercel.sh",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/d3": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.12.0",
    "d3-array": "^3.1.3",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-color": "^3.1.0",
    "d3-drag": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-fetch": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-path": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "d3-timer": "^3.0.1",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.10.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
}
